@article{pisor_incentives_2018,
	title = {Incentives for intergroup tolerance and association: {A} call for increased attention in evolutionary anthropological research},
	volume = {6},
	shorttitle = {Incentives for intergroup tolerance and association},
	abstract = {Primate individuals use a variety of strategies in intergroup encounters, from aggression to
23 tolerance; however, recent focus on the evolution of either warfare or peace has come at the
24 cost of characterizing this variability. We identify evolutionary advantages that may incentivize
25 tolerance toward extra-group individuals in humans and non-human primates, including
26 enhanced benefits in the domains of transfer, mating, and food acquisition. We highlight the role
27 these factors play in the flexibility of gorilla, chimpanzee, bonobo, and human behavior. Given
28 humans have an especially broad range of intergroup behavior, we explore how the human
29 foraging ecology, especially large geographic and temporal fluctuations in resource availability,
30 may have selected for a greater reliance on tolerant between-community relationships –
31 relationships reinforced by status acquisition and cultural institutions. We conclude by urging
32 careful, theoretically-motivated study of behavioral flexibility in intergroup encounters in
33 humans and the non-human great apes.},
