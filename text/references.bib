@article{eisenbruch_eisenbruch_2019,
	title = {Eisenbruch \& {Krasnow}\_Why warmth matters more than competence-new evolutionary models\_preprint {Aug} 16 2019},
	abstract = {Multiple lines of evidence suggest that there are two major dimensions of social perception, often called warmth and competence, and that warmth is prioritized over competence. Existing explanations for this prioritization argue that it is more consequential for an observer’s welfare than is competence. We develop a new explanation for the prioritization of warmth, based on cooperative partner choice. A review of the anthropological literature suggests that, over the course of human evolution, there was greater variance in the warmth than competence of potential cooperative partners, but greater variance in competence over time within cooperative relationships. We present agent-based models showing that these features of the ecology promote the evolution of the prioritization of warmth over competence, suggesting that this prioritization is due to an asymmetry in their distributions, not in their necessary consequences, per se. Future directions for the study of social perception and preferences more broadly are discussed.},
	author = {Eisenbruch, Adar and Krasnow, Max},
	year = {2019},
	keywords = {0-not relevant},
}

