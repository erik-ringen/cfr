@phdthesis{walker_evolution_2004,
	title = {Evolution of the human life history: ontogeny and behavior in two {South} {American} indigenous populations},
	shorttitle = {Evolution of the human life history},
	abstract = {This dissertation investigates why humans have low mortality rates, long slow-growing juvenile periods, and large brains. Of interest are two components of the human life history: (1) time and energy investments during the long juvenile period and (2) rewards of these investments during the long adult and post-reproductive periods. The age- and sex-dependency in anthropometrics, physical performance, economic production, and time allocation in two small scale economies are contrasted against that of chimpanzees to analyze the evolution of human life history characteristics. Two competing models, that of a complex foraging niche versus that of a complex social niche, are evaluated. Finally, an optimality model is developed and tested qualitatively and quantitatively with chimpanzee and human life history trajectories.},
	school = {The University of New Mexico},
	author = {Walker, Robert S.},
	year = {2004},
	keywords = {1-foraging, 2-children},
}

