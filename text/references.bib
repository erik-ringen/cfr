@article{apicella_voice_2009,
	title = {Voice {Pitch} {Alters} {Mate}-{Choice}-{Relevant} {Perception} in {Hunter}-{Gatherers}},
	volume = {276},
	issn = {0962-8452},
	url = {https://www.jstor.org/stable/30244939},
	abstract = {In humans, voice pitch is thought to be a cue of underlying quality and an important criterion for mate choice, but data from non-Western cultures have not been provided. Here we test attributions to and preferences for voices with raised and lowered pitch in hunter-gatherers. Using a forced-choice playback experiment, we found that both men and women viewed lower pitched voices in the opposite sex as being better at acquiring resources (e.g. hunting and gathering). While men preferred higher pitched women's voices as marriage partners, women showed no overall preference for voice pitch in men. However, women who were currently breastfeeding had stronger preferences for higher pitched male voices whereas women not currently breastfeeding preferred lower pitched voices. As testosterone is considered a costly signal associated with dominance, heritable immunity to infection and low paternal investment, women's preferences potentially reflect a trade-off between securing good genes and paternal investment. Men's preferences for higher pitched female voices are probably due to an evolved preference for markers of fecundity, reflected in voice pitch.},
	number = {1659},
	urldate = {2019-09-26},
	journal = {Proceedings: Biological Sciences},
